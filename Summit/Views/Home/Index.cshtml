    <section id="section-intro" class="section-wrapper about-event">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <h2>OUR OBJECTIVES</h2>
                    <p class="lead">
                        <ul style="list-style-type:disc; font-weight:normal">
                            <li>Increase technical skills, innovative thinking, and/or professional capabilities to further enable our Digital Transformation.</li>
                            <li>Increase understanding of company vision and customer needs, and provide key insights from WWLM, FRS, and Dell Technologies World.</li>
                            <li>Establish a strong career development network among participants and leaders.</li>
                        </ul>
                    </p>
                    <h2>PURPOSE</h2>
                    <p class="lead" style="font-weight:normal">
                        To dedicate time for personal and professional growth by providing accelerated learning focused on Strategy, Customers, and Innovation.
                    </p>
                </div>
                <div class="col-md-4">
                    <h4>SAVE THE DATES</h4>
                    <strong>
                        MAY 08-10, US
                        <br />JUNE 20-22, INDIA
                        <br />JUNE 26-27, MALAYSIA
                    </strong>
                </div>
            </div>
        </div>
    </section>
    <section id="section-agenda" class="section-wrapper section-agenda team gray-bg">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="section-title" style="margin-bottom:0px">
                        <h1>AGENDA</h1>
                    </div>
                </div>
            </div>
            <div class="row">
                @{
                    int i = 1;
                    foreach (Summit.Controllers.HomeController.EventDay day in ViewBag.Agenda)
                    {
                        if (ViewBag.Region == "US" && ViewBag.Program.ToLower() == "itlp (fy16-fy18)")
                        {
                            <div class="col-md-3">
                                <h3>Day @i - @day.date</h3>
                                <span style="font-size:17px;position:relative;top:-17px">@day.location</span>
                                <br /><span style="font-size:17px;position:relative;top:-17px">Attire: @day.attire</span>
                                @foreach (Summit.Controllers.HomeController.Groups group in day.Groups)
                                {
                                    <div class="session">
                                        @foreach (Summit.Controllers.HomeController.EventSession session in group.Sessions)
                                        {
                                            <time>@session.Time</time>
                                            <h3>
                                                @session.Title
                                                <span>@session.Extra</span>
                                            </h3>
                                        }
                                    </div>
                                }
                            </div>
                            i++;
                        }
                        else
                        {
                            <div class="col-md-4">
                                <h3>Day @i - @day.date</h3>
                                <span style="font-size:17px;position:relative;top:-17px">@day.location</span>
                                @if (ViewBag.Region == "US")
                                {
                                    <br /><span style="font-size:17px;position:relative;top:-17px">Attire: @day.attire</span>
                                }
                                @foreach (Summit.Controllers.HomeController.Groups group in day.Groups)
                                {
                                    <div class="session">
                                        @foreach (Summit.Controllers.HomeController.EventSession session in group.Sessions)
                                        {
                                            <time>@session.Time</time>
                                            <h3>
                                                @session.Title
                                                <span>@session.Extra</span>
                                            </h3>
                                        }
                                    </div>
                                }
                            </div>
                            i++;
                        }
                    }
                }
            </div>
        </div>
    </section>
